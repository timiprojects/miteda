<%- include layouts/header.ejs %>

        <div class="page-data">
            
            <form method="post" action="/api/events/add">
                <table cellpadding="11">
                <tr>
                    <td class="label">Event Name</td><td>: <input type="text" name="event_name" required></td>
                 </tr>
                 <tr>
                    <td class="label">Event Date</td><td>: <input type="date" name="event_date" required></td>
                 </tr>
                 <tr>
                    <td class="label">Event State</td><td>: <input type="text" name="event_state" required></td>
                 </tr>

                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>
            
        </div>
        
<%- include layouts/footer.ejs %>
